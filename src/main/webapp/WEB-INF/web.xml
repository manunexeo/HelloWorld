<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:shemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
   version="3.0">
      <display-name>Lab Nexeo DevOps Web Application</display-name>
      <servlet>
         <servlet-name>NexDevOpsServlet</servlet-name>
         <servlet-class>org.example.HelloServlet</servlet-class>
      </servlet>

      <servlet-mapping>
         <servlet-name>NexDevOpsServlet</servlet-name>
         <url-pattern>/</url-pattern>
      </servlet-mapping>
</web-app>
